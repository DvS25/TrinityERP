                    <div class="col-md-4">
                        <div class="pull-right" style="text-align:right;">
                            <span id="lblTimerCounter" class="bold fs-12 m-l-xxs"></span>
                            <input type="checkbox" id="chkAutoRefresh" class="icheck" style="padding-top: 5px;" autocomplete="off">
                            <label class="form-control-label" for="chkAutoRefresh" style="padding-left:5px"> Auto Refresh</label>
                        </div>
                    </div>
                </div>
                <div class="col-md-4" style="text-align:right;">
                    <div class="pull-right" style="width: 25px;">
                        <span id="lblTimerCounter" class="bold fs-12 m-l-xxs"></span>
                    </div>

                </div>
            </div>
            @*<div class="col-md-12">
                    <div id="dx_switchAccyearOpening"></div>
                </div>*@
            <div class="form-group row">
                <div class="col-md-12 table-responsive" style="padding:12px 0px;">
                    <table id="TableYearEndProcess" class="table table-bordered table-striped table-responsive" style="width:100%;">
                        <thead>
                            <tr>
                                <th style="text-align:center;width:10%">From Acc Year</th>
                                <th style="text-align:center;width:10%">To Acc Year</th>
                                <th style="text-align:center;width:14%">Start Date</th>
                                <th style="text-align:center;width:14%">End Date</th>
                                <th style="text-align:center;width:8%">Total Accounts</th>
                                <th style="text-align:center;width:8%">Complete Accounts</th>
                                @*<th style="text-align:center;width:8%">Total Products</th>
