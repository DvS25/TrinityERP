                        <p style="display:inline;margin-left: 10px;">
                            <input type="radio" id="Best" value="Best" name="radio-group">
                            <label for="Best">Best</label>
                        </p>
                        <p style="display:inline;margin-left: 10px;">
                            <input type="radio" id="Great"  value="Great" name="radio-group">
                            <label for="Great">Great</label>
                        </p>
                            </div>
                     
                      
                    </div>
                   
                </div>
                <div class="form-group row">
                    <label class="col-sm-2 form-control-label">1. Reference By</label>
                    <div class="col-sm-4">
                        <input class="form-control" type="text" id="Referecebyone" placeholder="Person Name">
                    </div>
                    <label class="col-sm-2 form-control-label">Notes</label>
                    <div class="col-sm-4">
                        <input class="form-control" type="text" id="Companynameone" placeholder="Notes">
                    </div>
                </div>
                <div class="form-group row">
