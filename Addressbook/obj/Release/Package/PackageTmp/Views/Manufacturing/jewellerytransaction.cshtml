                            <thead style="background-color: #eee;">
                                <tr>
                                    <th width="30%" class="text-center">File</th>
                                    <th width="50%" class="text-center">Description</th>
                                    <th width="20%" class=""></th>
                                </tr>
                            </thead>
                            <tbody id="tbody_AttachmentsList"></tbody>
                        </table>
                    </div>
